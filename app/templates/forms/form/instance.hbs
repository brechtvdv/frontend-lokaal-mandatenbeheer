{{page-title "Instance"}}
{{breadcrumb
  "Bewerk"
  " "
  this.model.form.id
  " "
  "instance"
  route="forms.form.new"
}}
<div class="au-c-body-container au-c-body-container--scroll">
  <div class="au-o-box">
    <SemanticForms::Instance
      @instanceId={{this.model.instanceId}}
      @form={{this.model.form}}
      @onSave={{this.onSave}}
      @onCancel={{this.onCancel}}
      @showHistory={{this.model.form.withHistory}}
    />
  </div>
</div>