{{! template-lint-disable no-unused-block-params }}
<AuTable>
  <:header>
    <tr>
      {{#each this.columns as |column|}}
        <th class="skeleton-text"><p class="skeleton-text"></p></th>
      {{/each}}
    </tr>
  </:header>
  <:body>
    {{#each this.rows as |row|}}
      <tr>
        {{#each this.columns as |column|}}
          <td><p class="skeleton-text"></p></td>
        {{/each}}
      </tr>
    {{/each}}
  </:body>
</AuTable>