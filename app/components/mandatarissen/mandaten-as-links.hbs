<div {{did-insert (perform this.setup)}}>
  {{#if (and this.setup.isRunning (eq this.mandatenAsLinks.length 0))}}
    <p class="skeleton-text"></p>
  {{else}}
    {{#each this.mandatenAsLinks as |mandaatAsLink index|}}
      <AuLink @route={{mandaatAsLink.route}} @models={{mandaatAsLink.model}}>
        {{mandaatAsLink.label}}{{#if
          (not-eq index (sub this.mandatenAsLinks.length 1))
        }},{{/if}}
      </AuLink>
    {{/each}}
  {{/if}}
</div>